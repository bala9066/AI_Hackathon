# Environment Variables for AI Hardware Pipeline
# Copy this file to .env and fill in your actual API keys

# OpenAI API Key (for GPT-4 integration)
OPENAI_API_KEY=sk-your-openai-key-here

# Anthropic API Key (for Claude integration)
ANTHROPIC_API_KEY=sk-ant-your-anthropic-key-here

# DigiKey API Credentials (for component search)
DIGIKEY_API_KEY=your-digikey-api-key
DIGIKEY_CLIENT_ID=your-digikey-client-id

# Mouser API Key (for component search)
MOUSER_API_KEY=your-mouser-api-key

# n8n Authentication
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=admin123

# Optional: Change timezone
GENERIC_TIMEZONE=Asia/Kolkata
