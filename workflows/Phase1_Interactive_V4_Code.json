                    }
                ]
            ]
        },
        "Prepare Search": {
            "main": [
                [
                    {
                        "node": "DigiKey API",
                        "type": "main",
                        "index": 0
                    },
                    {
                        "node": "Mouser API",
                        "type": "main",
                        "index": 0
                    }
                ]
            ]
        },
        "DigiKey API": {
            "main": [
                [
                    {
                        "node": "Merge Components",
                        "type": "main",
